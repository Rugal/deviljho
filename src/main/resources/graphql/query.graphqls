type Query {
  tags: [Tag!]!
  items: [Item!]!
  storages: [FridgeStorage!]!
}

type Item {
  iid: Int!
  name: String!
  createAt: Int!
  updateAt: Int!
}

type Tag {
  tid: Int!
  name: String!
  createAt: Int!
  updateAt: Int!
}

type ItemTag {
  itid: Int!
  name: String!
  item: Item!
  tag: Tag!
  createAt: Int!
  updateAt: Int!
}

type FridgeStorage {
  fsid: Int!
  quantity: Int!
  item: Item!
  createAt: Int!
  updateAt: Int!
}
